<div class='container'>
  <div class='row'>
    <div class="col-sm-12 col-md-8 col-lg-5 px-5 px-sm-0 mx-auto">
      <h1>編集</h1>
      <%= render 'layouts/errors', obj: @post %>
      <%= form_with model: @post do |f| %>
        <h4>年度</h4>
        <%= f.text_field :year %>
        <h4>月</h4>
        <%= f.text_area :month %>
        <%= f.submit '編集' %>
      <% end %>
    </div>
  </div>
</div>